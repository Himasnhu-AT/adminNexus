{
  "name": "@adminnexus/monorepo",
  "version": "0.0.1",
  "description": "Alternative of Notion, with cool features and AI functionality",
  "author": {
    "name": "Himanshu",
    "email": "hyattherate2005@gmail.com",
    "url": "https://github.com/Himasnhu-AT"
  },
  "main": "index.js",
  "scripts": {
    "pre-commit": "pnpm format && pnpm lint && turbo run build && pnpm test",
    "local:dev": "pnpm i && turbo run local-link && turbo run build",
    "local:prod": "pnpm i && turbo run local-link && turbo run build && turbo run prod",
    "prod:dev": "pnpm i && turbo run build",
    "prod:prod": "pnpm i && turbo run build && turbo run prod",
    "backend:dev": "turbo run dev --filter=@adminnexus/backend",
    "backend:prod": "turbo run prod --filter=@adminnexus/backend",
    "backend:build": "turbo run build --filter=@adminnexus/backend",
    "frontend:dev": "turbo run dev --filter=@adminnexus/web",
    "frontend:prod": "turbo run prod --filter=@adminnexus/web",
    "frontend:build": "turbo run build --filter=@adminnexus/web",
    "npm-publish": "turbo run prepublishOnly && turbo run npm-publish",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "test:stress": "turbo run test:stress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Himasnhu-AT/noterAI.git"
  },
  "keywords": [
    "Editor",
    "Notion",
    "OpenSource",
    "notes"
  ],
  "packageManager": "pnpm@9.11.0",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Himasnhu-AT/noterAI/issues"
  },
  "homepage": "https://github.com/Himasnhu-AT/noterAI#readme",
  "devDependencies": {
    "turbo": "^2.1.3"
  }
}
